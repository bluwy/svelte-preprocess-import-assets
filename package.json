{
	"name": "svelte-preprocess-import-assets",
	"description": "Import assets in markup",
	"version": "1.0.1",
	"license": "MIT",
	"author": "Bjorn Lu",
	"type": "module",
	"types": "index.d.ts",
	"typesVersions": {
		"*": {
			"*": [
				"src/*"
			]
		}
	},
	"exports": {
		".": {
			"types": "./src/index.d.ts",
			"default": "./src/index.js"
		}
	},
	"files": [
		"src"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/bluwy/svelte-preprocess-import-assets.git"
	},
	"bugs": {
		"url": "https://github.com/bluwy/svelte-preprocess-import-assets/issues"
	},
	"keywords": [
		"svelte",
		"preprocess",
		"import",
		"assets"
	],
	"scripts": {
		"lint": "prettier \"**/*.{js,ts,css,md,svelte}\" --check --cache",
		"format": "prettier \"**/*.{js,ts,css,md,svelte}\" --write --cache",
		"test": "node tests/index.js",
		"test-update": "node tests/index.js"
	},
	"packageManager": "pnpm@8.6.3",
	"dependencies": {
		"estree-walker": "^3.0.3",
		"magic-string": "^0.30.0",
		"svelte-parse-markup": "^0.1.2"
	},
	"peerDependencies": {
		"svelte": "^3.37.0 || ^4.0.0"
	},
	"devDependencies": {
		"prettier": "^2.8.7",
		"prettier-plugin-svelte": "^2.10.1",
		"sass": "^1.61.0",
		"svelte": "^4.2.1",
		"svelte-preprocess": "^5.0.4",
		"typescript": "^5.2.2",
		"uvu": "^0.5.6"
	}
}
